{
  "name": "terminal-ssh",
  "displayName": "Terminal SSH",
  "description": "Terminal SSH",
  "version": "0.1.0",
  "publisher": "sailhenz",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ssh-to-server"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "ssh-to-server",
        "title": "SSH to server"
      }
    ],
    "keybindings": [
      {
        "command": "ssh-to-server",
        "key": "ctrl+.",
        "mac": "cmd+.",
        "when": "terminalFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "eslint": "^4.11.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}
